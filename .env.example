# ============================================================================
# DNALang Quantum Research Platform - Environment Variables
# ============================================================================
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control!

# ============================================================================
# Supabase Configuration (Required)
# ============================================================================
# Get these from: https://supabase.com/dashboard/project/dnculjsqwigkivykedcf/settings/api

# Public URL (safe to expose to client)
NEXT_PUBLIC_SUPABASE_URL=https://dnculjsqwigkivykedcf.supabase.co

# Anonymous/Public key (safe to expose to client)
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_anon_key_here

# Service Role Key (SECRET - Server-side only, DO NOT EXPOSE TO CLIENT)
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.your_service_role_key_here

# ============================================================================
# IBM Quantum Configuration (Optional - for direct hardware access)
# ============================================================================
# Get your token from: https://quantum.ibm.com/account
# Or create QNET.json in ~/Desktop/QNET.json

IBM_QUANTUM_TOKEN=your_ibm_quantum_api_token_here
IBM_QUANTUM_CHANNEL=ibm_cloud

# ============================================================================
# Vercel Deployment (Optional - for CI/CD)
# ============================================================================
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_org_id_here
VERCEL_PROJECT_ID=your_project_id_here

# ============================================================================
# Application Configuration
# ============================================================================
# Base URL for your deployment
NEXT_PUBLIC_APP_URL=https://quantumlm-vercel-d996hiq5a-dnalang-67efe71c.vercel.app

# Node environment
NODE_ENV=production

# ============================================================================
# ΛΦ Tensor Framework Constants
# ============================================================================
# Universal Memory Constant (DO NOT CHANGE)
NEXT_PUBLIC_LAMBDA_PHI=2.176435e-8

# Canon II Propulsion Physics Constants
NEXT_PUBLIC_THETA_DEG=51.84
NEXT_PUBLIC_TAU_OMEGA_BASELINE=25411096.57
NEXT_PUBLIC_TAU_OMEGA_TARGET=50000000

# ============================================================================
# Feature Flags (Optional)
# ============================================================================
NEXT_PUBLIC_ENABLE_AURA_ORCHESTRATOR=true
NEXT_PUBLIC_ENABLE_RESEARCH_LAB=true
NEXT_PUBLIC_ENABLE_ENTERPRISE_PORTAL=true
NEXT_PUBLIC_ENABLE_QUANTUM_HARDWARE=true
NEXT_PUBLIC_ENABLE_REAL_TIME_TELEMETRY=true

# ============================================================================
# Development (Optional)
# ============================================================================
# Set to 'development' for local dev, 'production' for deployment
NEXT_PUBLIC_ENV=production

# Enable debug logging
DEBUG=false

# ============================================================================
# Security Notes
# ============================================================================
# 1. NEVER commit .env.local with real credentials
# 2. Service role key grants admin access - keep secret
# 3. Rotate keys if accidentally exposed
# 4. Use separate keys for dev/staging/production
# ============================================================================
