{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "regions": ["iad1"],
  "env": {
    "QS_UED_PALS_VERSION": "1.0.0",
    "UNIFIED_PLATFORM": "true"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-QS-UED-PALS",
          "value": "dna::}{::lang"
        }
      ]
    }
  ]
}
